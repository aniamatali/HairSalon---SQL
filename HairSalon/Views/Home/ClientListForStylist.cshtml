<p>@Model["stylist"].GetName()</p>

@foreach (var client in Model["client"])
{
<p>@client.GetName()</p>
<p>@client.GetHours()</p>
}

<form action="/Stylists/@Model["stylist"].GetId()/Delete" method="post">
  <button type="submit" class="btn btn-success">Remove Employee</button>
</form>

<a href="/Stylists">Employee Directory</a>
<br>
<a href="/Stylists/@Model["stylist"].GetId()/clients/new">Add Client</a>
